{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
        "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "skipLibCheck": true
  },
  "typedocOptions": {
    "entryPoints": ["./src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "readme": "README.md",
    "exclude": ["**/topologies/**", "**/node_modules/**"]
  }
}